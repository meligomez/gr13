<script>
	function obtenerValorDeLaFormular (){
		var selectElm = document.getElementById('IndicadoresSelect');
		var seleccionDelSelect=selectElm.options[selectElm.selectedIndex].text;
		var arrayIndicador = seleccionDelSelect.split(" || ")
		var formula = arrayIndicador[1]
		nombreDeLaEmpresa =  document.getElementById("nombre-de-la-empresa").textContent,
		periodoDesde = document.getElementById("periodo-desde").textContent,
		periodoHasta = document.getElementById("periodo-hasta").textContent;
		
		//formula = formula.replace("/", "%2F").trim();%252F
		formula = formula.replace("/", "%252F").trim();
		periodoDesde = periodoDesde.replace("/", "%252F").trim();
		periodoHasta = periodoHasta.replace("/", "%252F").trim();
		window.location.href = "/obtenerValorIndicador/" + nombreDeLaEmpresa + "/" + periodoDesde + "/" + periodoHasta + "/" + formula;
 		
	
    };

	function obtenerUPDATEindicador (){
